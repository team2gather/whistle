    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.0",
