{
  "name": "whistle",
  "version": "1.0.0",
  "description": "payment backend",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "watch-server": "nodemon --watch 'src/**/*' -e ts,tsx --exec npm run start-ts",
    "start-ts": "ts-node --files ./src/app.ts",
    "reset-db": "node ./sql/reset_db",
    "deploy": "git push heroku master",
    "test": "jest",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "postpublish": "git push origin master --follow-tags",
    "build-ts": "npm run clean && tsc",
    "watch-ts": "tsc -w",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/team2gather/whistle.git"
  },
  "engines": {
    "node": ">=8.10.x"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/team2gather/whistle/issues"
  },
  "homepage": "https://github.com/team2gather/whistle#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "escape-html": "^1.0.3",
    "knex": "^0.15.2",
    "koa": "^2.5.2",
    "koa-basic-auth": "^3.0.0",
    "koa-body": "^4.0.4",
    "koa-bodyparser": "^4.2.1",
    "koa-bouncer": "^6.0.4",
    "koa-compose": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-csrf": "^3.0.6",
    "koa-helmet": "^4.0.0",
    "koa-logger": "^3.2.0",
    "koa-passport": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.8.2",
    "koa-static": "^5.0.0",
    "moment": "^2.22.2",
    "node-sass": "^4.9.3",
    "passport-google-oauth": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "pg": "^7.4.3",
    "pg-extra": "^1.1.0",
    "querystring": "^0.2.0",
    "react": "^16.4.2",
    "react-template-render": "^1.1.0",
